    <style name="AppTheme" parent="Theme.AppCompat.DayNight.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="preferenceTheme">@style/PreferenceThemeOverlay.v14.Material</item>
        <item name="cardViewStyle">@style/CardViewStyle</item>
        <item name="coordinatorLayoutStyle">@style/Widget.Support.CoordinatorLayout</item>
        <item name="floatingActionButtonStyle">@style/Widget.Design.FloatingActionButton</item>
    </style>
    <style name="CardViewStyle" parent="CardView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">3dp</item>
        <item name="cardCornerRadius">5dp</item>
    </style>
    <style name="CardViewTitle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">top|center</item>
        <item name="android:layout_marginTop">10dp</item>
        <!--<item name="android:autoSizeMaxTextSize">18sp</item>
        <item name="android:autoSizeMinTextSize">14sp</item>
        <item name="android:autoSizeStepGranularity">1dp</item>-->
        <item name="android:gravity">center_horizontal</item>
        <item name="android:lines">1</item>
        <item name="android:textColor">@color/cardview_title_color</item>
        <item name="android:textSize">18sp</item>
    </style>
    <style name="InfoButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">25sp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:adjustViewBounds">false</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:cropToPadding">false</item>
    </style>
    <style name="MiniLineChart">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginTop">0dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:layout_marginBottom">0dp</item>
        <item name="android:minHeight">50dp</item>
    </style>
    <style name="PrimaryWeightMeasure" parent="Widget.AppCompat.TextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_gravity">center_horizontal|center|clip_vertical</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center_vertical|center_horizontal</item>
        <item name="android:inputType">numberSigned|numberDecimal</item>
        <item name="android:lines">1</item>
        <item name="android:maxWidth">100dp</item>
        <item name="android:maxLines">1</item>
        <item name="android:textColor">@color/enabled_color</item>
        <item name="android:textSize">30sp</item>
    </style>
    <style name="WeightIndexStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical|center_horizontal</item>
        <item name="android:maxWidth">100dp</item>
        <item name="android:maxLines">1</item>
        <item name="android:textColor">@color/index_color</item>
        <item name="android:textSize">20sp</item>
    </style>
    <style name="Widget.AppCompat.Button.AddButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">8dp</item>
        <item name="android:background">@color/add_button_background</item>
    </style>
    <style name="Widget.AppCompat.Button.Borderless.DetailsButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:adjustViewBounds">false</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:drawableStart">@drawable/ic_baseline_list_alt_24px</item>
        <item name="android:minHeight">0dp</item>
        <item name="android:paddingTop">0dp</item>
        <item name="android:paddingBottom">0dp</item>
    </style>
